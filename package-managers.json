{
  "atom": {
    "install": "apm install {{name}}{{#version}}@{{version}}{{/version}}",
    "download": "https://www.atom.io/api/packages/{{name}}/versions/{{version}}/tarball",
    "url": "https://atom.io/packages/{{name}}"
  },
  "biicode": {
    "url": "https://www.biicode.com/{{name}}/{{version}}"
  },
  "bower": {
    "install": "bower install {{name}} {{#version}}#{{version}}{{/version}}"
  },
  "cargo": {
    "download": "https://crates.io/api/v1/crates/{{name}}/{{version}}/download",
    "url": "https://crates.io/crates/{{name}}/{{version}}"
  },
  "clojars": {
    "url": "https://clojars.org/{{name}}/{{#version}}/versions/{{version}}{{/version}}"
  },
  "cocoapods": {
    "url": "http://cocoapods.org/pods/{{name}}",
    "documentation": "http://cocoadocs.org/docsets/{{name}}/{{version}}"
  },
  "dub": {
    "install": "dub fetch {{name}}{{#version}} --version {{version}}{{/version}}",
    "url": "http://code.dlang.org/packages/{{name}}/{{#version}}/{{version}}{{/version}}"
  },
  "jam": {
    "install": "jam install {{name}}{{#version}}@{{version}}{{/version}}",
    "download": "http://jamjs.org/repository/{{name}}/{{name}}-{{version}}.tar.gz",
    "url": "http://jamjs.org/packages/#/details/{{name}}/{{version}}"
  },
  "npm": {
    "install": "npm install {{name}}{{#version}}@{{version}}{{/version}}",
    "download": "http://registry.npmjs.org/{{name}}/-/{{name}}-{{version}}.tgz",
    "url": "https://www.npmjs.com/package/{{name}}"
  },
  "rubygems": {
    "install": "gem install {{name}} {{#version}}-v {{version}}{{/version}}",
    "download": "https://rubygems.org/downloads/{{name}}-{{version}}.gem",
    "url": "https://rubygems.org/gems/{{name}}{{#version}}/versions/{{version}}{{/version}}",
    "documentation": "http://www.rubydoc.info/gems/{{name}}/{{version}}"
  }
}
