{
  "atom": {
    "install": "apm install {{name}}{{#version}}@{{version}}{{/version}}",
    "download": "https://www.atom.io/api/packages/{{name}}/versions/{{version}}/tarball",
    "url": "https://atom.io/packages/{{name}}"
  },
  "biicode": {
    "url": "https://www.biicode.com/{{name}}/{{version}}"
  },
  "bower": {
    "install": "bower install {{name}} {{#version}}#{{version}}{{/version}}"
  },
  "cargo": {
    "download": "https://crates.io/api/v1/crates/{{name}}/{{version}}/download",
    "url": "https://crates.io/crates/{{name}}/{{version}}"
  },
  "clojars": {
    "url": "https://clojars.org/{{name}}/{{#version}}/versions/{{version}}{{/version}}"
  },
  "cocoapods": {
    "url": "http://cocoapods.org/pods/{{name}}",
    "documentation": "http://cocoadocs.org/docsets/{{name}}/{{version}}"
  },
  "cpan": {
    "url": "https://metacpan.org/release/{{name}}"
  },
  "cran": {
    "download": "https://cran.r-project.org/src/contrib/{{name}}_{{version}}.tar.gz",
    "url": "http://cran.r-project.org/web/packages/{{name}}/index.html",
    "documentation": "http://cran.r-project.org/web/packages/{{name}}/{{name}}.pdf"
  },
  "dub": {
    "install": "dub fetch {{name}}{{#version}} --version {{version}}{{/version}}",
    "url": "http://code.dlang.org/packages/{{name}}/{{#version}}/{{version}}{{/version}}"
  },
  "elm": {
    "install": "elm-package install {{name}} {{version}}",
    "url": "http://package.elm-lang.org/packages/{{name}}/{{#version}}/{{version}}{{/version}}{{^version}}latest{{/version}}"
  },
  "emacs": {
    "download": "http://melpa.org/packages/{{name}}-{{version}}.tar",
    "url": "http://melpa.org/#/{{name}}"
  },
  "go": {
    "install": "go get {{name}}",
    "documentation": "http://godoc.org/{{name}}",
    "url": "http://go-search.org/view?id={{name}}"
  },
  "hackage": {
    "install": "cabal install {{name}}{{#version}}-{{version}}{{/version}}",
    "download": "http://hackage.haskell.org/package/{{name}}-{{version}}/{{name}}-{{version}}.tar.gz",
    "url": "http://hackage.haskell.org/package/{{name}}{{#version}}-{{version}}{{/version}}"
  },
  "hex": {
    "url": "https://hex.pm/packages/{{name}}/{{version}}",
    "documentation": "http://hexdocs.pm/{{name}}/{{version}}"
  },
  "jam": {
    "install": "jam install {{name}}{{#version}}@{{version}}{{/version}}",
    "download": "http://jamjs.org/repository/{{name}}/{{name}}-{{version}}.tar.gz",
    "url": "http://jamjs.org/packages/#/details/{{name}}/{{version}}"
  },
  "julia": {
    "url": "http://pkg.julialang.org/?pkg={{name}}&ver=release"
  },
  "meteor": {
    "install": "meteor add {{name}}{{#version}}@={{version}}{{/version}}"
  },
  "nimble": {
    "install": "nimble install {{name}}{{#version}}@#{{version}}{{/version}}"
  },
  "npm": {
    "install": "npm install {{name}}{{#version}}@{{version}}{{/version}}",
    "download": "http://registry.npmjs.org/{{name}}/-/{{name}}-{{version}}.tgz",
    "url": "https://www.npmjs.com/package/{{name}}"
  },
  "nuget": {
    "install": "Install-Package {{name}}{{#version}} -Version {{version}}{{/version}}",
    "url": "https://www.nuget.org/packages/{{name}}/{{version}}"
  },
  "packagist": {
    "url": "https://packagist.org/packages/{{name}}#{{version}}"
  },
  "rubygems": {
    "install": "gem install {{name}} {{#version}}-v {{version}}{{/version}}",
    "download": "https://rubygems.org/downloads/{{name}}-{{version}}.gem",
    "url": "https://rubygems.org/gems/{{name}}{{#version}}/versions/{{version}}{{/version}}",
    "documentation": "http://www.rubydoc.info/gems/{{name}}/{{version}}"
  }
}
